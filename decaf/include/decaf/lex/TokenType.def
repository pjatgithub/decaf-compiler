#ifndef TOKEN
#define TOKEN(token, tokenStr)
#endif

#ifndef PUNCTUATOR
#define PUNCTUATOR(token, tokenStr)
#endif

#ifndef KEYWORD
#define KEYWORD(token, tokenStr)
#endif

TOKEN(UNKNOWN, nullptr)
TOKEN(EOF, nullptr)
TOKEN(IDENTIFIER, "IDENTIFIER")
TOKEN(INT_LITERAL, "INTLITERAL")
TOKEN(CHAR_LITERAL, "CHARLITERAL")
TOKEN(STRING_LITERAL, "STRINGLITERAL")
TOKEN(BOOLEAN_LITERAL, "BOOLEANLITERAL")

PUNCTUATOR(LEFT_SQUARE, "[")
PUNCTUATOR(RIGHT_SQUARE, "]")
PUNCTUATOR(LEFT_PAREN, "(")
PUNCTUATOR(RIGHT_PAREN, ")")
PUNCTUATOR(LEFT_BRACE, "{")
PUNCTUATOR(RIGHT_BRACE, "}")
PUNCTUATOR(SEMICOLON, ";")
PUNCTUATOR(COMMA, ",")
PUNCTUATOR(EQUAL, "=")
PUNCTUATOR(PLUS_EQUAL, "+=")
PUNCTUATOR(MINUS_EQUAL, "-=")
PUNCTUATOR(PLUS_PLUS, "++")
PUNCTUATOR(MINUS_MINUS, "--")
PUNCTUATOR(EQUAL_EQUAL, "==")
PUNCTUATOR(EXCLAM, "!")
PUNCTUATOR(EXCLAM_EQUAL, "!=")
PUNCTUATOR(PLUS, "+")
PUNCTUATOR(MINUS, "-")
PUNCTUATOR(ASTERISK, "*")
PUNCTUATOR(SLASH, "/")
PUNCTUATOR(PERCENT, "%")
PUNCTUATOR(AMPER_AMPER, "&&")
PUNCTUATOR(PIPE_PIPE, "||")
PUNCTUATOR(LESS, "<")
PUNCTUATOR(GREATER, ">")
PUNCTUATOR(LESS_EQUAL, "<=")
PUNCTUATOR(GREATER_EQUAL, ">=")

KEYWORD(IMPORT, "import")
KEYWORD(VOID, "void")
KEYWORD(INT, "int")
KEYWORD(BOOL, "bool")
KEYWORD(IF, "if")
KEYWORD(FOR, "for")
KEYWORD(WHILE, "while")
KEYWORD(RETURN, "return")
KEYWORD(BREAK, "break")
KEYWORD(CONTINUE, "continue")
KEYWORD(LEN, "len")

